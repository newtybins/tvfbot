{
    "name": "tvfbot",
    "description": "The Venting Forest's Discord bot!",
    "version": "1.0.0",
    "private": true,
    "main": "build/index.js",
    "author": "newt",
    "license": "GLP-3.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-nodemon",
        "beta": "npm run dev",
        "prod": "cross-env NODE_ENV=production ts-nodemon",
        "pm2": "NODE_ENV=production pm2 start node build/src/index.js",
        "build": "tsc",
        "clean": "rimraf build",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint src --ext .ts --fix",
            "git add"
        ]
    },
    "dependencies": {
        "axios": "0.21.1",
        "canvas": "2.6.1",
        "chart.js": "2.9.4",
        "chartjs-node-canvas": "3.0.6",
        "common-tags": "1.8.0",
        "cron": "^1.8.2",
        "cross-env": "7.0.3",
        "discord-akairo": "^8.1.0",
        "discord.js": "12.5.1",
        "dotenv": "8.2.0",
        "imgur-uploader": "4.2.1",
        "jimp": "0.16.1",
        "lodash": "4.17.20",
        "logdna-winston": "3.0.4",
        "moment": "2.29.1",
        "mongoose": "5.11.13",
        "ms": "2.1.3",
        "nanoid": "3.1.20",
        "novelcovid": "3.0.0",
        "ordinal": "^1.0.3",
        "pastebin-js": "1.0.6",
        "pretty-bytes": "5.5.0",
        "systeminformation": "4.34.9",
        "timeout": "0.2.1",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@trongnd/ts-nodemon": "1.0.3",
        "@types/common-tags": "1.8.0",
        "@types/lodash": "4.14.168",
        "@types/ms": "0.7.31",
        "@types/node": "14.14.22",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint": "^7.26.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "rimraf": "3.0.2",
        "typescript": "4.1.3"
    }
}
