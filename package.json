        "mongoose": "^5.6.9",
        "ms": "^2.1.2",
        "pretty-bytes": "^5.3.0",
        "systeminformation": "^4.14.4",
        "winston": "3.2.0",
        "winston-error": "^1.0.1",
        "winston-raven-sentry": "^2.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/mongoose": "^5.5.12",
        "@types/node": "^12.7.1",
        "cross-env": "^5.2.0",
        "del": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-typescript": "^5.0.1",
        "gulp-uglify": "^3.0.2",
        "husky": "^3.0.3",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "rimraf": "^2.6.3",
        "typescript": "^3.5.3"
