{
	"name": "tvfbot",
	"description": "The Venting Forest's Discord bot!",
	"version": "1.0.0",
	"private": true,
	"main": "dist/index.js",
	"author": "newtykins",
	"license": "GLP-3.0",
	"scripts": {
		"dev": "cross-env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 ts-nodemon",
		"beta": "npm run dev",
		"prod": "npm run build && npm run pm2:start",
		"pm2:start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 pm2 start node dist/index.js",
		"pm2:pause": "pm2 stop",
		"pm2:stop": "pm2 delete",
		"pm2:restart": "pm2 restart",
		"build": "tsc",
		"clean": "rimraf dist",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"prisma:pull": "prisma db pull",
		"prisma:generate": "prisma generate",
		"prisma": "npm run prisma:pull && npm run prisma:generate"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint src --ext .ts --fix",
			"git add"
		]
	},
	"dependencies": {
		"@catte_/pastebin.js": "^2.0.1",
		"@prisma/client": "^2.28.0",
		"@sapphire/decorators": "^2.2.0",
		"@sapphire/discord.js-utilities": "^2.0.2",
		"@sapphire/framework": "^1.0.2",
		"axios": "0.21.1",
		"canvas": "2.8.0",
		"colorthief": "^2.3.2",
		"common-tags": "1.8.0",
		"cron": "^1.8.2",
		"cross-env": "7.0.3",
		"discord.js": "12.5.3",
		"dotenv": "8.2.0",
		"human-readable-ids": "^1.0.4",
		"jimp": "0.16.1",
		"lodash": "4.17.21",
		"logdna-winston": "3.0.5",
		"moment": "2.29.1",
		"ms": "2.1.3",
		"ordinal": "^1.0.3",
		"pluralize": "^8.0.0",
		"pretty-bytes": "5.6.0",
		"request": "^2.88.2",
		"systeminformation": "5.7.7",
		"timeout": "0.2.1",
		"winston": "3.3.3"
	},
	"devDependencies": {
		"@trongnd/ts-nodemon": "1.0.3",
		"@types/common-tags": "1.8.1",
		"@types/lodash": "4.14.171",
		"@types/ms": "0.7.31",
		"@types/node": "16.3.2",
		"@types/timeout": "^0.2.0",
		"@typescript-eslint/eslint-plugin": "^4.28.3",
		"@typescript-eslint/parser": "^4.28.3",
		"eslint": "^7.30.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.0.1",
		"prisma": "^2.28.0",
		"rimraf": "3.0.2",
		"typescript": "4.3.5"
	}
}
