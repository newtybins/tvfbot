{
    "name": "tvfbot",
    "description": "The Venting Forest's Discord bot!",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "author": "newtykins",
    "license": "GLP-3.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 ts-nodemon",
        "beta": "npm run dev",
        "prod": "npm run build && npm run pm2:start",
        "pm2:start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 pm2 start node dist/index.js",
        "pm2:pause": "pm2 stop",
        "pm2:stop": "pm2 delete",
        "pm2:restart": "pm2 restart",
        "build": "tsc",
        "clean": "rimraf dist",
        "prisma": "prisma db pull && prisma generate"
    },
    "dependencies": {
        "@ayanobot/rankcard": "^2.0.2",
        "@catte_/pastebin.js": "^2.0.1",
        "@prisma/client": "^3.0.2",
        "axios": "0.21.4",
        "common-tags": "1.8.0",
        "cron": "^1.8.2",
        "cross-env": "7.0.3",
        "discord-akairo": "^8.1.0",
        "discord.js": "12.5.3",
        "dotenv": "10.0.0",
        "human-readable-ids": "^1.0.4",
        "jimp": "0.16.1",
        "lodash": "4.17.21",
        "logdna-winston": "4.0.0",
        "moment": "2.29.1",
        "ms": "3.0.0-canary.1",
        "ordinal": "^1.0.3",
        "pluralize": "^8.0.0",
        "pretty-bytes": "5.6.0",
        "request": "^2.88.2",
        "systeminformation": "5.9.3",
        "timeout": "0.2.1",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@trongnd/ts-nodemon": "1.0.3",
        "@types/common-tags": "1.8.1",
        "@types/lodash": "4.14.173",
        "@types/ms": "0.7.31",
        "@types/node": "16.9.3",
        "@types/timeout": "^0.2.0",
        "prisma": "^3.0.2",
        "rimraf": "3.0.2",
        "typescript": "4.4.3"
    }
}
