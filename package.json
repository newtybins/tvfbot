{
    "name": "tvfbot",
    "description": "The Venting Forest's Discord bot!",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "author": "newtykins",
    "license": "GLP-3.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 ts-nodemon",
        "beta": "npm run dev",
        "prod": "npm run build && npm run pm2:start",
        "pm2:start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 pm2 start node dist/index.js",
        "pm2:pause": "pm2 stop",
        "pm2:stop": "pm2 delete",
        "pm2:restart": "pm2 restart",
        "build": "tsc",
        "clean": "rimraf dist",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint src --ext .ts --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@catte_/pastebin.js": "^2.0.1",
        "axios": "0.21.1",
        "canvas": "2.6.1",
        "colorthief": "^2.3.2",
        "common-tags": "1.8.0",
        "cron": "^1.8.2",
        "cross-env": "7.0.3",
        "discord-akairo": "^8.1.0",
        "discord.js": "12.5.1",
        "dotenv": "8.2.0",
        "jimp": "0.16.1",
        "lodash": "4.17.20",
        "logdna-winston": "3.0.4",
        "moment": "2.29.1",
        "mongoose": "5.11.13",
        "ms": "2.1.3",
        "nanoid": "3.1.20",
        "ordinal": "^1.0.3",
        "pluralize": "^8.0.0",
        "pretty-bytes": "5.5.0",
        "request": "^2.88.2",
        "systeminformation": "4.34.9",
        "timeout": "0.2.1",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@trongnd/ts-nodemon": "1.0.3",
        "@types/common-tags": "1.8.0",
        "@types/lodash": "4.14.168",
        "@types/ms": "0.7.31",
        "@types/node": "14.14.22",
        "@types/timeout": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint": "^7.26.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "rimraf": "3.0.2",
        "typescript": "4.1.3"
    }
}
