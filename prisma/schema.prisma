generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES")
}

model Blacklist {
  id     String  @id
  reason String?
}

model User {
  id          String   @id
  level       Int      @default(1)
  xp          Int      @default(1)
  stickyRoles String[]
}

model Private {
  ownerID     String    @unique
  reason      String?
  requestedAt DateTime? @db.Timestamptz(6)
  startedAt   DateTime? @db.Timestamptz(6)
  textID      String?
  voiceID     String?
  id          Int       @id @default(autoincrement())
}
