generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id    String @id(map: "users_pk") @unique(map: "users_id_uindex")
  level Int    @default(1)
  xp    Int    @default(0)

  @@map("Users")
}
